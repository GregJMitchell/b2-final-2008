<p><%= @doctor.name %></p>
<p><%= @doctor.specialty %></p>
<p><%= @doctor.university %></p>
<p><%= @doctor.hospital.name %></p>
<% @doctor.patients.each do |patient| %>
  <div id="patient-<%= patient.id %>">
    <p><%= patient.name %></p>
    <%= button_to "Remove Patient", "/doctors/#{@doctor.id}/remove_patient", params: {patient_id: "#{patient.id}"},method: "delete"%>
  </div>
<% end %>